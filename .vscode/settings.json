{
  "cSpell.words": [
    "autodocs",
    "datasource",
    "esnext",
    "hookform",
    "latin",
    "multistatus",
    "nextjs",
    "paralleldrive",
    "planria",
    "PROPPATCH"
  ],
  "editor.codeActionsOnSave": {
    "source.fixAll": "always",
    "source.organizeImports": "always"
  },
  "editor.formatOnSave": true,
  "editor.defaultFormatter": "dbaeumer.vscode-eslint",
  "tailwindCSS.classAttributes": [
    "class",
    "className",
    "cn"
  ],
  "tailwindCSS.experimental.configFile": "tools/tailwindcss/tailwind.config.ts"
}