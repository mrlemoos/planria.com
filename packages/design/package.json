{
  "name": "@planria/design",
  "version": "0.0.0",
  "sideEffects": false,
  "license": "Apache-2.0",
  "exports": {
    "./avatar": {
      "types": "./src/avatar.tsx",
      "import": "./dist/avatar.mjs",
      "require": "./dist/avatar.js"
    },
    "./button": {
      "types": "./src/button.tsx",
      "import": "./dist/button.mjs",
      "require": "./dist/button.js"
    },
    "./css": {
      "types": "./src/css.ts",
      "import": "./dist/css.mjs",
      "require": "./dist/css.js"
    },
    "./logo": {
      "types": "./src/logo.tsx",
      "import": "./dist/logo.mjs",
      "require": "./dist/logo.js"
    },
    "./navigation": {
      "types": "./src/navigation.tsx",
      "import": "./dist/navigation.mjs",
      "require": "./dist/navigation.js"
    },
    "./theme": {
      "types": "./src/theme.tsx",
      "import": "./dist/theme.mjs",
      "require": "./dist/theme.js"
    },
    "./top-bar": {
      "types": "./src/top-bar.ts",
      "import": "./dist/top-bar.mjs",
      "require": "./dist/top-bar.js"
    }
  },
  "scripts": {
    "build": "tsup ./src",
    "dev": "tsup ./src --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "devDependencies": {
    "@planria/eslint-config": "workspace:*",
    "@planria/tsconfig": "workspace:*",
    "@types/react": "^18.2.61",
    "@types/react-dom": "^18.2.19",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@radix-ui/react-avatar": "^1.1.0",
    "@radix-ui/react-slot": "^1.1.0",
    "clsx": "^2.1.1",
    "react": "^18.2.0",
    "tailwind-merge": "^2.5.2",
    "tailwind-variants": "^0.2.1"
  }
}