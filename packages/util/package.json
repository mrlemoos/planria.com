{
  "name": "@planria/util",
  "version": "1.0.0",
  "description": "Utility functions used across Planria projects.",
  "sideEffects": false,
  "license": "Apache-2.0",
  "scripts": {
    "build": "tsup ./src",
    "dev": "tsup ./src --watch",
    "lint": "eslint . --max-warnings 0",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "test": "vitest run"
  },
  "exports": {
    "./assert": {
      "types": "./src/assert.ts",
      "import": "./dist/assert.mjs",
      "require": "./dist/assert.js"
    },
    "./crypto": {
      "types": "./src/crypto.ts",
      "import": "./dist/crypto.mjs",
      "require": "./dist/crypto.js"
    },
    "./currency": {
      "types": "./src/currency.ts",
      "import": "./dist/currency.mjs",
      "require": "./dist/currency.js"
    },
    "./date": {
      "types": "./src/date.ts",
      "import": "./dist/date.mjs",
      "require": "./dist/date.js"
    },
    "./errors": {
      "types": "./src/errors.ts",
      "import": "./dist/errors.mjs",
      "require": "./dist/errors.js"
    },
    "./http": {
      "types": "./src/http.ts",
      "import": "./dist/http.mjs",
      "require": "./dist/http.js"
    },
    "./logging": {
      "types": "./src/logging.ts",
      "import": "./dist/logging.mjs",
      "require": "./dist/logging.js"
    },
    "./objects": {
      "types": "./src/objects.ts",
      "import": "./dist/objects.mjs",
      "require": "./dist/objects.js"
    },
    "./proto": {
      "types": "./src/proto.ts",
      "import": "./dist/proto.mjs",
      "require": "./dist/proto.js"
    },
    "./runtime": {
      "types": "./src/runtime.ts",
      "import": "./dist/runtime.mjs",
      "require": "./dist/runtime.js"
    },
    "./strings": {
      "types": "./src/strings.ts",
      "import": "./dist/strings.mjs",
      "require": "./dist/strings.js"
    },
    "./uuid": {
      "types": "./src/uuid.ts",
      "import": "./dist/uuid.mjs",
      "require": "./dist/uuid.js"
    },
    "./zod": {
      "types": "./src/zod.ts",
      "import": "./dist/zod.mjs",
      "require": "./dist/zod.js"
    }
  },
  "devDependencies": {
    "@planria/eslint-config": "workspace:*",
    "@planria/tsconfig": "workspace:*",
    "@types/bcryptjs": "^2.4.6",
    "typescript": "^5.3.3",
    "tsup": "^8.3.0",
    "vitest": "^2.0.5"
  },
  "dependencies": {
    "@asteasolutions/zod-to-openapi": "^7.1.1",
    "@paralleldrive/cuid2": "^2.2.2",
    "bcryptjs": "^2.4.3",
    "dayjs": "^1.11.13",
    "form-data": "^4.0.0",
    "ky": "^1.7.1",
    "tslog": "^4.9.3",
    "zod": "^3.23.8"
  }
}